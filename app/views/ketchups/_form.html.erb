<%= simple_form_for [@trip, @ketchup] do |f| %>
  <%= f.association :user, collection: @friends, :label_method => :full_name, :value_method => :id, prompt: "Choose a friend" %>
  <%= f.input :date %>
  <%= f.input :start_time %>
  <%= f.input :duration, label: 'Duration (in min)', input_html: { value: 30, min: 30, step: 15 } %>
  <%= f.input :location %>
  <%= f.input :message %>
  <%= f.submit "Search your friends", class: "btn btn-flat" %>
<% end %>
